spring.application.name=cloud-config-service

#spring.cloud.config.server.git.uri：配置git仓库地址
#spring.cloud.config.server.git.searchPaths：配置仓库路径
#spring.cloud.config.label：配置仓库的分支
#spring.cloud.config.server.git.username：访问git仓库的用户名
#spring.cloud.config.server.git.password：访问git仓库的用户密码
#注意，这里不能写=https://github.com/liyouzhibj/media-system.git
spring.cloud.config.server.git.uri=https://github.com/liyouzhibj/media-system.git
#这个config是git里面的一级目录，propertis只能放在config目录下，如果配置文件放在config/aaa/sss.properties,找不到
spring.cloud.config.server.git.search-paths=config
spring.cloud.config.label=master
